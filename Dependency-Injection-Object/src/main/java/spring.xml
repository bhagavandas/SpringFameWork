<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id = "objRefByValue" class="com.example.demo.MathCheat"></bean>
  <bean
	id = "stu" class = "com.example.demo.Student">
	<property name="id" value="1001"></property>
	<property name="mathCheat" ref="objRefByValue">
	
	</property>
   </bean> 
   
  <bean id="anotherStudent" class="com.example.demo.AnotherStudent">
	<property name="cheat" ref="objRefByValue">
	
	</property>
	
	</bean>
</beans>